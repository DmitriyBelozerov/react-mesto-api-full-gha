{
  "env": {
    "browser": false,
    "commonjs": false,
    "es2021": false
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "warn",
    "func-names": "warn",
    "prefer-arrow-callback": "off",
    "consistent-return": "off"
  }
}
